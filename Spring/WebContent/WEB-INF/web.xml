<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>Spring</display-name>
	<welcome-file-list>
		<welcome-file>UserHome.html</welcome-file>
		<welcome-file>UserHome.htm</welcome-file>
		<welcome-file>UserHome.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>  
    <servlet-name>spring</servlet-name>  
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>  
    <load-on-startup>1</load-on-startup>  
</servlet>
	<servlet>
		<description>
		</description>
		<display-name>Report</display-name>
		<servlet-name>Report</servlet-name>
		<servlet-class>com.controller.Report</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ItemReport</display-name>
		<servlet-name>ItemReport</servlet-name>
		<servlet-class>com.controller.ItemReport</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>Report1</display-name>
		<servlet-name>Report1</servlet-name>
		<servlet-class>com.controller.Report1</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UserRegister</display-name>
		<servlet-name>UserRegister</servlet-name>
		<servlet-class>com.controller.UserRegister</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>Billing</display-name>
		<servlet-name>Billing</servlet-name>
		<servlet-class>com.controller.Billing</servlet-class>
	</servlet>  
<servlet-mapping>  
    <servlet-name>spring</servlet-name>  
    <url-pattern>*.html</url-pattern>  
</servlet-mapping>
<servlet-mapping>
	<servlet-name>Report</servlet-name>
	<url-pattern>/Report</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>ItemReport</servlet-name>
	<url-pattern>/ItemReport</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>Report1</servlet-name>
	<url-pattern>/Report1</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>UserRegister</servlet-name>
	<url-pattern>/UserRegister</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>Billing</servlet-name>
	<url-pattern>/Billing</url-pattern>
</servlet-mapping>  
</web-app>
